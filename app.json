{
  "expo": {
    "name": "Joobz - Cre8steve",
    "slug": "joobz-errands-and-services",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "joobz",
    "userInterfaceStyle": "automatic",
    "description": "",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#33CC66"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.cre8stevedev.joobz",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#33CC66"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c901adbd-762d-4842-93b7-7f2f17dec691"
      }
    }
  }
}
